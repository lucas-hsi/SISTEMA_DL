# Configuração Firebase - DL Auto Peças
# Copie este arquivo para .env e preencha com suas credenciais do Firebase

# Configurações do Firebase (projeto: dlautopecas-amb)
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=dlautopecas-amb.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=dlautopecas-amb
VITE_FIREBASE_STORAGE_BUCKET=dlautopecas-amb.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Configurações de desenvolvimento
# Defina como 'true' para usar emuladores Firebase em desenvolvimento
VITE_USE_FIREBASE_EMULATOR=false

# Configurações de ambiente
VITE_ENVIRONMENT=development